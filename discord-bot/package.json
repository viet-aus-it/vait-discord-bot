{
  "name": "@viet-aus-it/discord-bot",
  "version": "0.0.1",
  "description": "Discord chat bot for VAIT",
  "main": "build/index.js",
  "engines": {
    "node": "^14.0.0",
    "npm": "^6.0.0",
    "yarn": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viet-aus-it/vait-discord-bot.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viet-aus-it/vait-discord-bot"
  },
  "homepage": "https://github.com/viet-aus-it/vait-discord-bot#readme",
  "scripts": {
    "clean:all": "npm run prebuild && npm run pretest",
    "prebuild": "rimraf build",
    "build": "tsc",
    "build:watch": "tsc -w",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "yarn lint --fix",
    "format": "yarn lint --fix && prettier -w src/**/*",
    "pretest": "rimraf coverage",
    "test": "jest",
    "start": "ts-node-dev --watch src/index.ts",
    "serve": "node build/index.js"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "bufferutil": "^4.0.3",
    "discord.js": "^12.5.3",
    "discord/erlpack": "discord/erlpack",
    "sodium": "^3.0.2",
    "utf-8-validate": "^5.0.4",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.13",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.18.2",
    "faker": "^5.1.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.0.2"
  }
}
