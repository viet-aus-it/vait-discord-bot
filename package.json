{
  "name": "@viet-aus-it/discord-bot",
  "version": "2.12.1",
  "description": "Chat bot for VAIT Discord Server",
  "main": "build/index.js",
  "engines": {
    "node": "^22.0.0",
    "pnpm": "^10.0.0"
  },
  "packageManager": "pnpm@10.24.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viet-aus-it/vait-discord-bot.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viet-aus-it/vait-discord-bot"
  },
  "homepage": "https://github.com/viet-aus-it/vait-discord-bot#readme",
  "scripts": {
    "prepare": "husky",
    "typecheck": "tsc",
    "build": "tsdown",
    "build:typecheck": "pnpm typecheck && pnpm build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:fix:unsafe": "biome check --write --unsafe .",
    "format:biome": "biome format --write .",
    "ci": "biome ci .",
    "format": "pnpm format:biome && prisma format",
    "test": "dotenvx run -- vitest",
    "test:silent": "dotenvx run -- vitest --silent",
    "prisma:migrate": "dotenvx run -- prisma migrate dev",
    "prisma:gen": "dotenvx run -- prisma generate",
    "prisma:studio": "dotenvx run -- prisma studio",
    "deploy:command": "dotenvx run -- tsx scripts/deploy-guild-commands.ts",
    "delete:command": "dotenvx run -- tsx scripts/delete-guild-commands.ts",
    "delete:command-global": "dotenvx run -- tsx scripts/delete-global-commands.ts",
    "start:only": "dotenvx run -- tsx --import ./bin/telemetry.ts --watch ./bin/main.ts",
    "start": "pnpm prisma:migrate && pnpm prisma:gen && pnpm start:only",
    "build:referrals": "tsx scripts/build-referral-list.ts"
  },
  "dependencies": {
    "@axiomhq/winston": "^1.3.1",
    "@discordjs/rest": "2.0.0",
    "@dotenvx/dotenvx": "^1.51.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.70.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.212.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.212.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.212.0",
    "@opentelemetry/resources": "^2.5.1",
    "@opentelemetry/sdk-metrics": "^2.5.1",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@opentelemetry/sdk-trace-base": "^2.5.1",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@prisma/client-runtime-utils": "^7.0.1",
    "cowsay": "^1.6.0",
    "date-fns": "^4.1.0",
    "discord-api-types": "^0.38.34",
    "discord.js": "^14.25.1",
    "node-html-parser": "^7.0.1",
    "oxide.ts": "^1.1.0",
    "parse-duration": "^2.1.4",
    "winston": "^3.18.3",
    "wretch": "^3.0.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@faker-js/faker": "^10.1.0",
    "@testcontainers/postgresql": "^11.9.0",
    "@types/node": "^22.19.1",
    "@types/node-fetch": "^2.6.13",
    "@vitest/coverage-v8": "^4.0.14",
    "@vitest/ui": "^4.0.14",
    "esbuild": "^0.27.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.3",
    "prisma": "^7.0.1",
    "rolldown": "1.0.0-beta.29",
    "rollup-plugin-copy": "^3.5.0",
    "testcontainers": "^11.9.0",
    "tsdown": "^0.16.8",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14",
    "vitest-mock-extended": "^3.1.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@prisma/engines",
      "@prisma/client",
      "cpu-features",
      "esbuild",
      "msw",
      "prisma",
      "protobufjs",
      "ssh2"
    ]
  }
}
