{
  "name": "@viet-aus-it/discord-bot",
  "version": "2.2.0",
  "description": "Chat bot for VAIT Discord Server",
  "main": "build/index.js",
  "engines": {
    "node": "^16.0.0",
    "pnpm": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viet-aus-it/vait-discord-bot.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viet-aus-it/vait-discord-bot"
  },
  "homepage": "https://github.com/viet-aus-it/vait-discord-bot#readme",
  "scripts": {
    "prepare": "husky install",
    "clean:all": "pnpm prebuild && pnpm pretest",
    "typecheck": "tsc --noEmit",
    "build": "rimraf build && webpack",
    "build:typecheck": "pnpm typecheck && pnpm build",
    "lint": "esw --cache src/**/*.ts",
    "lint:fix": "pnpm lint -- --fix",
    "lint:watch": "pnpm lint -- --watch",
    "prettier": "prettier --cache -w src/**/*",
    "format": "pnpm lint:fix && pnpm prettier",
    "test": "jest",
    "test:silent": "jest --silent",
    "test:typecheck": "concurrently pnpm:test:silent pnpm:typecheck",
    "test:watch": "pnpm test -- --watchAll",
    "prisma:migrate": "prisma migrate dev",
    "prisma:gen": "prisma generate",
    "prisma:studio": "prisma studio",
    "deploy:command": "node -r @swc-node/register src/scripts/deploy-guild-commands.ts",
    "delete:command": "node -r @swc-node/register src/scripts/delete-guild-commands.ts",
    "start": "pnpm prisma:migrate && pnpm prisma:gen && pnpm build -- --watch"
  },
  "dependencies": {
    "@discordjs/rest": "^1.0.0",
    "@prisma/client": "4.1.0",
    "cowsay": "^1.5.0",
    "discord-api-types": "^0.33.3",
    "discord.js": "^14.0.3",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "node-fetch": "^2.6.7",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.3.0",
    "@jest/globals": "^28.1.3",
    "@jest/types": "^28.1.3",
    "@swc-node/register": "^1.5.1",
    "@swc/core": "^1.2.218",
    "@swc/helpers": "^0.4.3",
    "@swc/jest": "^0.2.22",
    "@types/node": "^18.0.6",
    "@types/node-fetch": "^2.6.2",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "concurrently": "^7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-watch": "^8.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "msw": "^0.44.2",
    "nodemon": "^2.0.19",
    "nodemon-webpack-plugin": "^4.8.1",
    "prettier": "^2.7.1",
    "prisma": "4.1.0",
    "rimraf": "^3.0.2",
    "swc-loader": "^0.2.3",
    "tree-kill": "^1.2.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
