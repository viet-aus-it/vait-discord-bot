{
  "name": "@viet-aus-it/discord-bot",
  "version": "2.1.0",
  "description": "Chat bot for VAIT Discord Server",
  "main": "build/index.js",
  "engines": {
    "node": "^16.0.0",
    "pnpm": "^7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viet-aus-it/vait-discord-bot.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viet-aus-it/vait-discord-bot"
  },
  "homepage": "https://github.com/viet-aus-it/vait-discord-bot#readme",
  "scripts": {
    "prepare": "husky install",
    "clean:all": "pnpm prebuild && pnpm pretest",
    "typecheck": "tsc --noEmit",
    "build": "rimraf build && webpack",
    "build:typecheck": "pnpm typecheck && pnpm build",
    "lint": "esw src --ext .ts",
    "lint:fix": "pnpm lint -- --fix",
    "lint:watch": "pnpm lint -- --watch",
    "prettier": "prettier -w src/**/*",
    "format": "pnpm lint:fix && pnpm prettier",
    "test": "jest",
    "test:silent": "jest --silent",
    "test:typecheck": "concurrently pnpm:test:silent pnpm:typecheck",
    "test:watch": "pnpm test -- --watchAll",
    "prisma:migrate": "prisma migrate dev",
    "prisma:gen": "prisma generate",
    "prisma:studio": "prisma studio",
    "deploy:command": "node -r @swc-node/register src/scripts/deploy-guild-commands.ts",
    "delete:command": "node -r @swc-node/register src/scripts/delete-guild-commands.ts",
    "start": "pnpm prisma:migrate && pnpm prisma:gen && pnpm build -- --watch"
  },
  "dependencies": {
    "@discordjs/builders": "^0.13.0",
    "@discordjs/rest": "^0.4.1",
    "@prisma/client": "3.14.0",
    "cowsay": "^1.5.0",
    "discord-api-types": "^0.33.3",
    "discord.js": "^13.7.0",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "node-fetch": "^2.6.7",
    "ramda": "^0.28.0",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "@jest/types": "^28.1.0",
    "@swc-node/register": "^1.5.1",
    "@swc/core": "^1.2.196",
    "@swc/helpers": "^0.3.17",
    "@swc/jest": "^0.2.21",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.39",
    "@types/node-fetch": "^2.6.1",
    "@types/ramda": "^0.28.13",
    "@types/webpack": "^5.28.0",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "concurrently": "^7.2.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-watch": "^8.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-mock-console": "^2.0.0",
    "lint-staged": "^13.0.0",
    "msw": "^0.42.0",
    "nodemon": "^2.0.16",
    "nodemon-webpack-plugin": "^4.7.1",
    "prettier": "^2.6.2",
    "prisma": "3.14.0",
    "rimraf": "^3.0.2",
    "swc-loader": "^0.2.1",
    "tree-kill": "^1.2.2",
    "ts-jest": "^28.0.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
